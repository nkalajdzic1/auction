{
  "name": "auction",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "heroku-postbuild": "cd client && npm install && --only=dev --no-shrinkwrap && npm run build",
    "start": "nodemon server/server.js",
    "react": "cd client && npm start",
    "dev": "concurrently \"npm run start\" \"npm run react\"",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo:all",
    "seed-all": "npx sequelize-cli db:seed:all",
    "make-model": "npx sequelize-cli model:generate --name _entity_name_ --attributes atribute:type,..."
  },
